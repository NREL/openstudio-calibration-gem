  <class_name>FansMultiplier</class_name>
  <display_name>Fans Multiplier</display_name>
  <description>This is a general purpose measure to calibrate Fans with a Multiplier.</description>
  <modeler_description>It will be used for calibration maximum flow rate, efficiency, pressure rise and motor efficiency. User can choose between a SINGLE Fan or ALL the Fans.</modeler_description>
  <arguments>
    <argument>
      <name>fan</name>
      <display_name>Apply the Measure to a SINGLE Fan, ALL the Fans or NONE.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>{b48061ed-58fa-4a2d-9d42-3758edb78dd4}</default_value>
      <choices>
        <choice>
          <value>{b48061ed-58fa-4a2d-9d42-3758edb78dd4}</value>
          <display_name>*All Fans*</display_name>
        </choice>
        <choice>
          <value>0</value>
          <display_name>*None*</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>max_flowrate_multiplier</name>
      <display_name>Multiplier for Maximum FlowRate.</display_name>
      <description>Multiplier for Maximum FlowRate.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>fan_efficiency_multiplier</name>
      <display_name>Multiplier for Fan Efficiency.</display_name>
      <description>Multiplier for Fan Efficiency.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>pressure_rise_multiplier</name>
      <display_name>Multiplier for Pressure Rise.</display_name>
      <description>Multiplier for Pressure Rise.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>motor_efficiency_multiplier</name>
      <display_name>Multiplier for Motor Efficiency.</display_name>
      <description>Multiplier for Motor Efficiency.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
  </arguments>
