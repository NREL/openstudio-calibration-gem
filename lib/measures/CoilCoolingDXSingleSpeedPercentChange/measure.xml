  <class_name>CoilCoolingDXSingleSpeedPercentChange</class_name>
  <display_name>Cooling Coils DX Single Speed Percent Change</display_name>
  <description>This is a general purpose measure to calibrate DX Cooling Coils with a Percent Change.</description>
  <modeler_description>It will be used for calibration of rated capacity and efficiency and parasitic loads. User can choose between a SINGLE coil or ALL the Coils.</modeler_description>
  <arguments>
    <argument>
      <name>coil</name>
      <display_name>Apply the Measure to a SINGLE DX Cooling Coil, ALL the DX Cooling Coils or NONE.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>{6d4cc6b3-8d52-4727-8fa3-6da413ff42a9}</default_value>
      <choices>
        <choice>
          <value>{6d4cc6b3-8d52-4727-8fa3-6da413ff42a9}</value>
          <display_name>*All DX Cooling Coils*</display_name>
        </choice>
        <choice>
          <value>0</value>
          <display_name>*None*</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>rated_cop_perc_change</name>
      <display_name>Percent Change for COP.</display_name>
      <description>Percent Change for COP.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>rated_cooling_capacity_perc_change</name>
      <display_name>Percent Change for coil cooling Capacity.</display_name>
      <description>Percent Change for coil cooling Capacity.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
  </arguments>
